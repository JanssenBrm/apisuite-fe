{
  "name": "apisuite-client-sandbox",
  "version": "1.0.0-alpha.0",
  "description": "APISuite sandbox package",
  "main": "dist/index.js",
  "engines": {
    "node": ">=10"
  },
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "mock-api": "json-server --watch __mocks__/db.json --port 9004",
    "prebuild": "yarn test && rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "pretest": "yarn lint && yarn check-types",
    "test": "start-server-and-test dev http://localhost:9001 cy:run",
    "commit": "git-cz",
    "prestart": "yarn build",
    "start": "serve -s dist -l 8080",
    "open-cov": "open coverage/lcov-report/index.html",
    "check-types": "tsc",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "cy": "cross-env NODE_ENV=test cypress open",
    "cy:run": "cross-env NODE_ENV=test cypress run"
  },
